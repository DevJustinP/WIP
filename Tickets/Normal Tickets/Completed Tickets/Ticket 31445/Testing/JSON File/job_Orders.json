{
  "job_module": "Orders",
  "job": "Import",
  "context": {
	"source": {
	  "database": "PRODUCT_INFO",
	  "server": "Talend"
	},
    "fields": {
      "order_date_c": "SorMaster.OrderDate",
      "name": "SorMaster.SalesOrder",
      "po_number_c": "SorMaster.CustomerPoNumber",
      "shipping_address_street_c": "SorMaster.ShipAddress1&SorMaster.ShipAddress2",
      "shipping_address_city_c": "SorMaster.ShipAddress3",
      "shipping_address_state_c": "SorMaster.ShipAddress4",
      "shipping_address_postalcode_c": "SorMaster.ShipPostalCode",
      "shipping_address_country_c": "SorMaster.ShipAddress5",
      "market_segment_c": "SorMaster.MarketSegment",
      "shipment_request_c": "SorMaster.ShipmentRequest",
      "branch_c": "SorMaster.Branch",
      "channel_c": "case statement based on SorMaster.Branch",
      "order_status_c": "SorMaster.OrderStatus",
      "noearlierthandate_c": "SorMaster.NoEarlierThanDate",
      "nolaterthandate_c": "SorMaster.NoLaterThanDate",
      "DocumentType": "SorMaster.DocumentType",
      "lookup_account_number": "SorMaster.Customer",
      "lookup_specifier_account_number": "SorMaster.Specifier",
      "lookup_purchaser_account_number": "SorMaster.Purchaser",
      "lookup_assigned_user_email": "SorMaster.Customer = ArCusomter.Customer | ArCustomer.Salesperson = SalSalesperson+.Salesperson | SalSalesperson+.CrmEmail",
      "lookup_salesperson1_email": "SorMaster.Customer = ArCusomter.Customer | ArCustomer.Salesperson1 = SalSalesperson+.Salesperson | SalSalesperson+.CrmEmail",
      "lookup_salesperson2_email": "SorMaster.Customer = ArCusomter.Customer | ArCustomer.Salesperson2 = SalSalesperson+.Salesperson | SalSalesperson+.CrmEmail",
      "lookup_salesperson3_email": "SorMaster.Customer = ArCusomter.Customer | ArCustomer.Salesperson3 = SalSalesperson+.Salesperson | SalSalesperson+.CrmEmail"
    }
  },
  "options": {
    "prevent_duplicates": "true",
    "retries_allowed": "300",
    "alert_on_failure": "true",
    "alert_on_completion": "false",
	"assigned_user_id":"<Meagans Sugar ID>"
  }
}
